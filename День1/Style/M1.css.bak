.header {
  overflow: hidden;
  background-color: #ADD8E6;
  padding: 20px 10px;
}

.header a {
  float: left;
  color: black;
  text-align: center;
  padding: 12px;
  text-decoration: none;
  font-size: 18px;
  line-height: 25px;
  border-radius: 4px;
}


.header a.logo {
  font-size: 25px;
  font-weight: bold;
}

.header a:hover {
  background-color: #ddd;
  color: black;
}


.header a.active {
  
  color: black;
}

.header-right {
  float: right;
}
.container{
	
	justify-content:center;
}
@media (max-width:768px){
	width: var(--somevar);
	  height: var(--somevar);
}
@media screen and (max-width: 320px) {
  .header a {
    float: none;
    display: block;
    text-align: left;
  }
  .header-right {
    float: none;
  }
  .container{
	 width:200px
  }
}
.logos{
	margin-top: 20px;
	width:250px;
	height:250px;
}
body{
	font-family: Geneva, Arial, Helvetica, sans-serif;
	}
	p{text-indent: 20px;}